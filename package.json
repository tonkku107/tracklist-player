{
  "name": "tracklist-player",
  "productName": "Tracklist Player",
  "version": "1.2.0",
  "description": "An audio player application which is designed for playing mixes. It displays the currently playing track based on the timestamps in a tracklist.",
  "main": "./out/main/index.js",
  "type": "module",
  "author": "tonkku107",
  "contributors": [
    "tonkku107"
  ],
  "license": "AGPL-3.0-or-later",
  "repository": "tonkku107/tracklist-player",
  "scripts": {
    "build": "electron-vite build",
    "build:linux": "yarn build && electron-builder --linux --config",
    "build:mac": "yarn build && electron-builder --mac --config",
    "build:win": "yarn build && electron-builder --win --config",
    "dev": "electron-vite dev",
    "format": "prettier --write .",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.4",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^9.2.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "music-metadata-browser": "^2.5.10",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "rss-parser": "^3.13.0",
    "uuid": "^9.0.1",
    "vite": "^5.2.11",
    "vite-plugin-node-polyfills": "^0.21.0"
  }
}
