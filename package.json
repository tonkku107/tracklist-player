{
  "name": "tracklist-player",
  "productName": "Tracklist Player",
  "version": "1.4.0-beta.0",
  "description": "An audio player application which is designed for playing mixes. It displays the currently playing track based on the timestamps in a tracklist.",
  "main": "./out/main/index.js",
  "type": "module",
  "author": "tonkku107",
  "contributors": [
    "tonkku107"
  ],
  "license": "AGPL-3.0-or-later",
  "repository": "tonkku107/tracklist-player",
  "scripts": {
    "build": "electron-vite build",
    "build:linux": "yarn build && electron-builder --linux --config",
    "build:mac": "yarn build && electron-builder --mac --config",
    "build:win": "yarn build && electron-builder --win --config",
    "dev": "electron-vite dev",
    "format": "prettier --write .",
    ".postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "discord-rpc": "^4.0.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@vitejs/plugin-react": "^5.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "music-metadata": "^11.9.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "react-window": "^2.1.1",
    "rss-parser": "^3.13.0",
    "uuid": "^13.0.0",
    "vite": "^7.1.6",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "packageManager": "yarn@1.22.22"
}
